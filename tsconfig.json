{
  "extends": "@quasar/app/tsconfig-preset",
  "no-floating-promises": true,
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/@types/node/index.d.ts",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": []
  }
}
